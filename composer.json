{
	"name": "mls/ki",
	"type": "library",
	"description": "ki framework for PHP: The non-intrusive framework",
	"require": {
		"phpmailer/phpmailer": ">=6.0.0rc5",
		"robloach/component-installer": "*",
		"components/jquery": "*",
		"components/jqueryui": ">=1.12",
		"php": ">=7.0.0"
	},
    "autoload": {
        "psr-4": { "mls\\ki\\": "src/" }
    },
	"extra": {
		"component": {
			"scripts": [
				"static/ki.js",
				"static/webshim/polyfiller.js"
			],
			"styles": [
				"static/ki.css"
			],
			"files": [
				"static/webshim/shims/*",
				"static/webshim/shims/*/*",
				"static/webshim/shims/*/*/*"
			],
			"name": "ki"
		}
	}
}
